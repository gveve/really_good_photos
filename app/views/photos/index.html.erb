
  <div class="row">
  <% @photos.each do |photo| %>
    <div class="col-sm-6">
      <div class="card">
        <div class="card-block">
        <img class="card-img-top" src="<%= photo["image_url"] %>" alt="Card image cap">
          <div class="card-body">
            <h4 class="card-title"><%=photo.name%></h4>
            <p class="card-text"><%=photo.description%></p>
          </div>
        </div>

        <div class="card-body">
          <p><%= link_to "photo link", photo_path(photo) %></p>
        </div>
      </div>
    </div>
  <% end %>
</div>


